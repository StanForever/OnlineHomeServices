        <div class="checkbox">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
            
        </div>
        <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
        <br><a href="#">Forgot Password</a>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp;<a href="#">Reset Password</a>
      </form>

    

    
